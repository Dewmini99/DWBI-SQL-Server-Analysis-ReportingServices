  <DataSets>
    <DataSet Name="Restaurant_Parameter_Dataset">
      <Query>
        <DataSourceName>Restaurant_Parameter</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@province">
            <Value>=Parameters!province.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT dm.primary_cuisiness, dm.cuisines, dr.AlternateRestaurantsID, dr.name, da.address, da.postalCode, da.city, da.province, da.country, fp.res_id, fp.Price, fp.discount
FROM     FactPrice AS fp INNER JOIN
                  DimMeal AS dm ON fp.MealKey = dm.MealSK INNER JOIN
                  DimRestaurants AS dr ON fp.RestaurantsKey = dr.RestaurantsSK INNER JOIN
                  DimAddress AS da ON fp.AddressKey = da.AddressSK
WHERE  (da.province = @province)</CommandText>
      </Query>
